  "devDependencies": {
    "@types/lodash": "^4.17.0",
    "@types/readline-sync": "^1.4.8",
    "@types/shelljs": "^0.8.14",
    "@types/yargs": "^17.0.32",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
